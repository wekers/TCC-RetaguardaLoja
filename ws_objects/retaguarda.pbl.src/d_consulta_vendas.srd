table(column=(type=long updatewhereclause=yes name=cod_vendedor dbname="movimento_saida.cod_vendedor" )
 column=(type=decimal(6) updatewhereclause=yes name=valor_total dbname="valor_total" )
 column=(type=char(255) updatewhereclause=yes name=usuarios_nome dbname="usuarios.nome" )
 column=(type=decimal(0) updatewhereclause=yes name=nr_clientes dbname="nr_clientes" )
 retrieve="  SELECT movimento_saida.cod_vendedor,   
         sum(~"movimento_saida~".~"valor_total~") as valor_total,   
         usuarios.nome,   
         count(id) as nr_clientes  
    FROM movimento_saida,   
         usuarios  
   WHERE ( movimento_saida.cod_vendedor = usuarios.cod ) and  
        ( CAST( ~"movimento_saida~".~"cod_vendedor~" as text) like :vendedor ) AND  
         ( ~"movimento_saida~".~"data_saida~" between :inicio and :final )   
GROUP BY movimento_saida.cod_vendedor,   
         usuarios.nome   
" arguments=(("inicio", date),("final", date),("vendedor", string))  sort="usuarios_nome A " )
